LOAD CSV WITH HEADERS FROM 'https://raw.githubusercontent.com/AcuoFS/acuo-allocation/master/test/testFiles/modify1AssetCostToTestAllocationAlgo.csv' 
AS line
WITH line
WHERE line.clientId IS NOT NULL

MATCH (c:Client {id:line.clientId})-[p:POSSESSES]->(a:Asset {id: line.id})-[is:IS_AVAILABLE_FOR]->(ag:Agreement {id:line.agreementId})

SET
	p.internalCost = p.oriInternalCost,
	p.opptCost = p.oriOpptCost,
	is.externalCost = is.oriExternalCost
REMOVE
	p.oriInternalCost,
	p.oriOpptCost,
	p.orExternalCost
